/*Questão 2. Solicitar 5 números, ao fim, imprimir o número maior e o número menor.*/

int[] numeros = new int[5];
for (int i = 0; i < numeros.Length; i++)
{
    Console.WriteLine("informe o numero");
    numeros[i] = int.Parse(Console.ReadLine());
}
int menorNumero = numeros.Min();
int maiorNumero = numeros.Max();
Console.WriteLine("Menor número: " + menorNumero);
Console.WriteLine("Maior número: " + maiorNumero);
